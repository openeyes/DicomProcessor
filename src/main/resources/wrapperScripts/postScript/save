bindedObjects.forEach(function(object){
        if(object.saveMethod == json){
                       putText(
                                object.attachmentMnemonic,
                                JSON.stringify(object.item),
                                object.attachmentType,
                                object.bodySite,
                                object.mimeType);
        } else if(object.saveMethod == blob) {
                putBlob(object.attachmentMnemonic,
                             object.blob,
                             object.attachmentType,
                             object.bodySite,
                             object.mimeType);
        }
                       });